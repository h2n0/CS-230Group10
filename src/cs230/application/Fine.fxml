<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="475.0" prefWidth="586.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs230.application.FineController">
   <children>
      <VBox prefHeight="54.0" prefWidth="586.0">
         <children>
            <Label prefHeight="41.0" prefWidth="232.0" text="Outstanding Fines">
               <font>
                  <Font size="29.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" top="10.0" />
               </VBox.margin>
            </Label>
         </children>
      </VBox>
      <HBox alignment="CENTER" prefHeight="57.0" prefWidth="586.0" spacing="15.0">
         <children>
            <Label alignment="BASELINE_LEFT" contentDisplay="CENTER" text="Search for student :">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="studentBox" />
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#handleSearchButton" text="Search" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#handleCancelButton" text="TEMPEditFine Cancel" />
            <Button mnemonicParsing="false" text="Back" />
         </children>
      </HBox>
      <TableView fx:id="tableView" editable="true" prefHeight="352.0" prefWidth="546.0">
        <columns>
          <TableColumn fx:id="UserName" prefWidth="240" text="UserName" />
          <TableColumn fx:id="Amount" prefWidth="168" text="Amount" />
          <TableColumn fx:id="EditColumn" prefWidth="135" text="Edit" />
        </columns>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" />
         </VBox.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </TableView>
   </children>
</VBox>
