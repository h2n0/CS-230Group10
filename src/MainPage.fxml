<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MainPageController">
   <center>
      <ScrollPane prefHeight="323.0" prefWidth="488.0" BorderPane.alignment="CENTER">
         <content>
            <Label prefHeight="51.0" prefWidth="74.0" text="HomePage" />
