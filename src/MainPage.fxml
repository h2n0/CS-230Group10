            <HBox prefHeight="51.0" prefWidth="82.0">
               <children>
                  <ImageView fitHeight="30.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true" />
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Label" />
                        <Label text="Label" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <TextField>
               <opaqueInsets>
